  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --env dev",
    "test": "nodemon server.js --env test",
    "prod": "nodemon server.js --env prod",
    "simplex": "artillery quick --count 40 --num 50 'http://localhost:8080/test/simplex' -o ./test/quick/simplex.json",
    "complex": "artillery quick --count 40 --num 50 'http://localhost:8080/test/complex' -o ./test/quick/complex.json",
    "performance": "artillery run ./test/flow/flow.yaml --output ./test/flow/flow.json",
    "report": "artillery report ./test/flow/flow.json",
    "image": "docker build -t fedemusci/projectbackend:1.0.4 .",
    "container": "docker run -p 8080:8080 fedemusci/projectbackend:1.0.4 .",
    "orchestra": "kubectl apply -f ./orchestra.yaml",
    "mocha": "mocha ./test/mocha",
    "chai": "mocha ./test/chai",
    "supertest": "mocha ./test/supertest"
  },
